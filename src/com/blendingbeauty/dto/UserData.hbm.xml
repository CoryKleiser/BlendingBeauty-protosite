<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.blendingbeauty.dto.UserData" table="" catalog="">
        <id name="userId" type="java.lang.Integer">
            <column name="USER_ID" />
            <generator class="identity" />
        </id>
        <property name="name" type="string">
            <column name="USER_NAME" length="255" not-null="false" unique="false" />
        </property>
        <property name="userStatus" type="string">
            <column name="USER_STATUS" length="255" not-null="false" unique="false" />
        </property>
        <property name="pointCount" type="java.lang.Integer">
            <column name="USER_POINTS" length="255" not-null="false" unique="false" />
        </property>
    </class>
</hibernate-mapping>